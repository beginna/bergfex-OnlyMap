{
  "name": "Inject OnlyMap",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Injecting OnlyMap stuff",
  "homepage_url": "https://github.com/beginna/bergfex-OnlyMap",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_title": "Inject OnlyMap",
    "default_icon": "icons/icon.png"
  },
  "permissions": [
    "tabs",
    "*://www.bergfex.at/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "bergfexOnlyMap@guerrillamail.org"
    }
  }
}